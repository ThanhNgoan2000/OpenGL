        d2.quack();
        d2.display();
        d2.swim();
        d2.fly();
        System.out.println("------------------");
        d3.quack();
        d3.display();
        d3.swim();
        d3.fly();
        System.out.println("------------------");
        d4.quack();
        d4.display();
        d4.swim();
        d4.fly();
        
        
